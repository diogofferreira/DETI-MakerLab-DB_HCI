<Page x:Class="DETI_MakerLab.RequisitionPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:DETI_MakerLab"
      mc:Ignorable="d" 
      d:DesignHeight="628" d:DesignWidth="1196"
      Background="White"
      Title="RequisitionPage">

    <Grid>
        <Label Content="Associated to"  Style="{StaticResource descriptionStyle}" HorizontalAlignment="Left" Margin="75,481,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.573,0.678" Width="144" Height="45"/>
        <Label Content="Requisition Content"  Style="{StaticResource descriptionStyle}" HorizontalAlignment="Left" Margin="32,77,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.573,0.678" Width="215" Height="36"/>
        <TextBlock x:Name="project_name" HorizontalAlignment="Left" Height="31" Margin="273,487,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="690" FontSize="20" RenderTransformOrigin="0.5,0.5"/>
        <ListBox x:Name="content_list" HorizontalAlignment="Left" Height="285" Margin="273,77,0,0" VerticalAlignment="Top" Width="690"/>
        <Button x:Name="go_back" Content="Go Back" Style="{StaticResource goBackStyle}" Margin="990,569,26,10" Click="go_back_Click"/>
    </Grid>
</Page>
