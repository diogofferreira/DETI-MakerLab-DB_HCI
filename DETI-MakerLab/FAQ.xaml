<Page x:Class="DETI_MakerLab.FAQ"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:DETI_MakerLab"
      mc:Ignorable="d" 
      d:DesignHeight="704" d:DesignWidth="1358"
      Title="FAQ">

    <Grid>
        <Image Source="images/question.png" HorizontalAlignment="Left" Height="210" Margin="512,10,0,0" VerticalAlignment="Top" Width="263"/>
        <Button x:Name="go_back" Content="Go Back" Style="{StaticResource mainWindowDmlButtonStyle}" HorizontalAlignment="Left" Margin="1203,629,0,0" VerticalAlignment="Top" Width="145" Height="65" Click="go_back_Click"/>
        <TextBox x:Name="faq_text" Foreground="White" FontSize="16" ScrollViewer.VerticalScrollBarVisibility="Visible" Style="{StaticResource TextBlockStyle}" TextWrapping="Wrap" HorizontalAlignment="Left" Margin="200,255,0,49" Height="400" Width="960"/>
        <Button x:Name="change_button" Content="Staff FAQ" Style="{StaticResource mainWindowDmlButtonStyle}" HorizontalAlignment="Left" Margin="55,438,0,0" VerticalAlignment="Top" Click="Button_Click"/>
    </Grid>
</Page>
